<div class="container">
    <div class="form-page clearfix">
        <div class="row pull-left">
            <div class="col-sm-5">
                <div class="row">
                    <div class="col-sm-4">
                        <div class="form-group">
                            <label for="segmentYear" class="control-label">Segment</label>
                            <select name="" class="form-control control-select-div" id="">
                                <option value="" data-show="show-equity" data-id="contract-equity">Equity</option>
                                <option value="" data-show="show-equity" data-id="contract-derivative">Derivative</option>
                                <option value="" data-show="show-equity" data-id="contract-derivative">Commodity</option>
                                <option value="" data-show="show-equity" data-id="contract-derivative">Currency</option>
                            </select>
                        </div>
                    </div>
                    <div class="col-sm-4">
                        <div class="form-group">
                            <label for="segmentYear" class="control-label">P&L</label>
                            <select name="" class="form-control control-inputs" id=""> 
                                <option value="Realised P&amp;L" data-columns="8" data-value="disabled-inputs" data-disabled="false">Realised
                                    P&amp;L</option>
                                <option value="UnRealised P&amp;L" data-columns="8" data-value="disabled-inputs" data-disabled="true">UnRealised
                                    P&amp;L</option>
                            </select>
                        </div>
                    </div>
                    <div class="col-sm-4">
                        <div class="form-group disabled-inputs show-equity contract-equity">
                            <label for="segmentYear" class="control-label">financial year</label>
                            <select name="" class="form-control" id="segmentYear" disabled>
                                <% for (var i =14; i <=21;  i++ ) { %>
                                <option value="2016-2017">20<%= i %>-20<%= i+1 %></option>
                                <% } %>
                            </select>
                        </div>
                        <div class="form-group disabled-inputs show-equity contract-derivative nodisplay">
                            <label for="segmentYear" class="control-label">financial year</label>
                            <select name="" class="form-control" id="segmentYear">
                                <% for (var i =14; i <=21;  i++ ) { %>
                                <option value="2016-2017">20<%= i %>-20<%= i+1 %></option>
                                <% } %>
                            </select>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-sm-4">
                <div class="row">
                    <div class="col-sm-6">
                        <div class="form-group disabled-inputs">
                            <label for="toDate" class="control-label">From Date</label>
                            <div class="input-icon input-icon-right">
                                <i class="fa fa-calendar icons"></i>
                                <input type="text" class="form-control pickDate" id="toDate" placeholder="DD/MM/YYYY">
                            </div>
                        </div>
                    </div>
                    <div class="col-sm-6">
                        <div class="form-group disabled-inputs">
                            <label for="toDate" class="control-label">TO Date</label>
                            <div class="input-icon input-icon-right">
                                <i class="fa fa-calendar icons"></i>
                                <input type="text" class="form-control pickDate" id="fromDate" placeholder="DD/MM/YYYY">
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-sm-2">
                <div class="form-group">
                    <label for="segmentYear" class="control-label">Search</label>
                    <div class="input-icon input-icon-right">
                        <i class="fa fa-search icons"></i>
                        <input type="text" class="form-control search-input" data-columns="2" placeholder="Search">
                    </div>
                </div>
            </div>
            <div class="col-sm-1">
                <div class="form-group with-btn">
                    <button class="btn btn-theme hoverable waves-effect btn-block waves-light">Submit</button>
                </div>
            </div>
        </div>
        <!-- <div class="categories-artical with-bg with-margin pull-right">
            <div class="btn-header">
                <ul class="btn-header-link">
                    <li class=""><a href="#"><span class="fa fa-download icons"></span> <span
                                class="text">Download</span></a>
                    </li>
                    <li class=""><a href="#"><span class="fa fa-envelope-o icons"></span> <span
                                class="text">Email</span></a>
                    </li>
                </ul>
            </div>
        </div> -->
    </div>
    <div class="categories-artical with-bg">
        <div class="btn-header">
            <ul class="btn-header-link">
                <li class=""><a href="#" class="export-excel" data-table="#table-n01" data-ignorecolumn="3,4"><span
                            class="fa fa-file-excel-o icons"></span> <span class="text">Excel</span></a></li>
                <li class=""><a href="#"><span class="fa fa-file-pdf-o icons"></span> <span class="text">PDF</span></a>
                </li>
                <li class=""><a href="#"><span class="fa fa-envelope-o icons"></span> <span
                            class="text">Email</span></a></li>
            </ul>
        </div>
        <div class="dt-table-responsive">
            <table id="holdingstable" class="table  table-symbols text-right" width="100%">
                <thead>
                    <tr>
                        <th width="30%" class="with-icon">Symbol </th>
                        <th class="with-icon text-right">QTY </th>
                        <th class="with-icon text-right">Buy Avg </th>
                        <th class="with-icon text-right">Buy Value </th>
                        <th class="with-icon text-right">Sell Avg </th>
                        <th class="with-icon text-right">Sell Value </th>
                        <th class="with-icon text-right">Prev Close </th>
                        <th class="with-icon text-right tabledin-taxt">Realised P&L </th>
                    </tr>
                </thead>
                <tbody>
                    <% for(var j=0; j < 10; j++) { %>
                    <tr>
                        <td class="text-left">
                            <a href="dashboard.html" class="bold text-gray"><span>
                                    <% if (j % 2 ) { %>
                                    WIPRO
                                    <%} else if (j % 3) { %>
                                    TATAMOTORS
                                    <%} else { %>
                                    TIINDIA
                                    <% } %>
                                </span>
                            </a>
                        </td>
                        <td>1000<%= j*3 %></td>
                        <td><%= j+1*3 %>31.79</td>
                        <td><%= j+2*3 %>31.79</td>
                        <td>NA</td>
                        <td>NA</td>
                        <td><%= j+1*4 %>0.<%= j+1*4 %></td>
                        <td> <% if (j % 2 ) { %>
                            <span class="text-danger"><%= j+1*4 %>0<%= j+1*8 %></span>
                            <%} else { %>
                            <span class="text-success"><%= j+1*4 %>0<%= j+1*8 %></span>
                            <% } %>
                        </td>
                    </tr>
                    <% } %>
                </tbody>
            </table>
        </div>
    </div>
</div>