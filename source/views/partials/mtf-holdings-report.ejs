<div class="container">
    <div class="categories-artical with-bg">
        <div class="btn-header">
            <ul class="btn-header-link">
                <li class=""><a href="#" class="export-excel" data-table="#table-n01" data-ignorecolumn="3,4"><span
                            class="fa fa-file-excel-o icons"></span> <span class="text">Excel</span></a></li>
                <li class=""><a href="#"><span class="fa fa-file-pdf-o icons"></span> <span
                            class="text">PDF</span></a></li>
                            <li class=""><a href="#"><span class="fa fa-envelope-o icons"></span> <span
                                class="text">Email</span></a></li>
            </ul>
        </div>
        
        <div class="dt-table-responsive">
            <table id="holdingstable" class="table  table-symbols text-right" width="100%">
                <thead>
                    <tr>
                        <th width="30%" class="with-icon">Symbol </th>
                        <th class="with-icon text-right">QTY </th>
                        <th class="with-icon text-right">Prev Close </th>
                        <th class="with-icon text-right">Market Val </th>

                    </tr>
                </thead>
                <tbody>
                    <% for(var j=0; j < 30; j++) { %>
                    <tr>
                        <td class="text-left">
                            <a href="dashboard.html" class="bold text-gray"><span>
                                    <% if (j % 2 ) { %>
                                    WIPRO
                                    <%} else if (j % 3) { %>
                                    TATAMOTORS
                                    <%} else { %>
                                    TIINDIA
                                    <% } %>
                                </span>
                            </a>
                        </td>
                        <td>1000<%= j*3 %> <small class="text-lock"><%= j+1*65 %>00 <span
                                    class="fa fa-lock icons"></span></small>
                        </td>
                        <td><%= j+1*3 %>31.79</td>
                        <td><%= j+1*3 %>00</td>

                    </tr>
                    <% } %>
                </tbody>
            </table>
        </div>
        <p class="text-note"><span class="fa fa-lock icons"></span> Pleadge Shares</p>
    </div>
</div>