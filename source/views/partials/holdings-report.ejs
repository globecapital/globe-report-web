<div class="container">
    <div class="categories-artical with-bg">
        <div class="btn-header">
            <ul class="btn-header-link">
                <li class=""><a href="#" class="export-excel" data-table="#table-n01" data-ignorecolumn="3,4"><span
                            class="fa fa-file-excel-o icons"></span> <span class="text">Excel</span></a></li>
                <li class=""><a href="#"><span class="fa fa-file-pdf-o icons"></span> <span class="text">PDF</span></a>
                </li>
                <li class=""><a href="#"><span class="fa fa-envelope-o icons"></span> <span
                            class="text">Email</span></a></li>
            </ul>
        </div>

        <div class="dt-table-responsive">
            <table id="holdingstable" class="table  table-symbols text-right" data-sortable="true" width="100%">
                <thead>
                    <tr>
                        <th width="20%" class="with-icon">Symbol</th>
                        <th class="with-icon text-right">NRML&nbsp;QTY</th>
                        <th class="with-icon text-right">MTF&nbsp;QTY</th>
                        <th class="with-icon text-right">Total&nbsp;QTY</th>
                        <th class="with-icon text-right">Buy&nbsp;Avg</th>
                        <th class="with-icon text-right">Buy&nbsp;Value</th>
                        <th class="with-icon text-right">Prev&nbsp;Close</th>
                        <th class="with-icon text-right">Market&nbsp;Val</th>
                        <th class="with-icon text-right">Unrealised&nbsp;P&L</th>
                        <th class="with-icon text-right">Day&nbsp;P&L</th>
                        <th class="with-icon text-right">Day&nbsp;P&L&nbsp;Chg</th>
                    </tr>
                </thead>
                <tbody>
                    <% for(var j=0; j < 30; j++) { %>
                    <tr>
                        <td class="text-left">
                            <a href="dashboard.html" class="bold text-gray"><span>
                                    <% if (j % 2 ) { %>
                                    WIPRO
                                    <%} else if (j % 3) { %>
                                    TATAMOTORS
                                    <%} else { %>
                                    TIINDIA
                                    <% } %>
                                </span>
                            </a>
                        </td>
                        <td><%= j+1*3 %>31.79</td>
                        <td><%= j+1*3 %>31.79</td>
                        <td>1000<%= j*3 %><small class="text-lock"><%= j+1*65 %>00 <span
                                    class="fa fa-lock icons"></span></small>
                        </td>
                        <td><%= j+1*3 %>31.79</td>
                        <td><%= j+2*3 %>31.79</td>
                        <td>1<%= j+1*3 %>0</td>
                        <td><%= j+1*3 %>00</td>
                        <td class="bg-gray">
                            <% if (j % 2 ) { %>
                            <span class="text-danger"><%= j+1*3 %>0<%= j+1*3 %></span>
                            <%} else { %>
                            <span class="text-success"><%= j+1*3 %>0<%= j+1*3 %></span>
                            <% } %>
                        </td>
                        <td>
                            <% if (j % 2 ) { %>
                            <span class="text-danger"><%= j+1*3 %>0<%= j+1*3 %></span>
                            <%} else { %>
                            <span class="text-success"><%= j+1*3 %>0<%= j+1*3 %></span>
                            <% } %>
                        </td>
                        <td>
                            <div class="dropdown-inner">
                                <% if (j % 2 ) { %>
                                <span class="text-danger">+<%= j+1*4 %>.<%= j+1*8 %>%</span>
                                <%} else { %>
                                <span class="text-success">+<%= j+1*4 %>.<%= j+1*8 %>%</span>
                                <% } %>
                                <div class="dropdown dropdown-table">
                                    <button class="button" id="dLabel" type="button" data-toggle="dropdown"
                                        aria-haspopup="true" aria-expanded="false">
                                        <span class="fa fa-ellipsis-v icons-caret"></span>
                                    </button>
                                    <ul class="dropdown-menu" aria-labelledby="dLabel">
                                        <li><a href="#" data-toggle="modal" data-target="#viewTradesPop"
                                                data-target-content="#tradesPop<%= j%>">View Trades</a></li>
                                        <li><a href="#">Pledge for margin</a></li>
                                        <li><a href="#">Cancel Pledge Request</a></li>
                                    </ul>
                                    <div id="tradesPop<%= j%>" class="nodisplay">
                                        <div class="preparation-contant">
                                            <div class="symbol"><% if (j % 2 ) { %>
                                                WIPRO
                                                <%} else if (j % 3) { %>
                                                TATAMOTORS
                                                <%} else { %>
                                                TIINDIA
                                                <% } %></div>
                                            <div class="isin">ine121465641</div>
                                            <div class="total-qty">1000<%= j*3 %></div>
                                            <div class="price"><%= j+1*3 %>31.79</div>
                                            <div class="prv-close">1<%= j+1*3 %>0</div>
                                            <div class="date">14 Jun 2021</div>
                                            <div class="age-days">935</div>
                                            <div class="profit-loss"> <% if (j % 2 ) { %>
                                                <span class="text-danger"><%= j+1*3 %>0<%= j+1*3 %></span>
                                                <%} else { %>
                                                <span class="text-success"><%= j+1*3 %>0<%= j+1*3 %></span>
                                                <% } %>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </td>
                    </tr>
                    <% } %>
                </tbody>
            </table>
        </div>
        <p class="text-note"><span class="fa fa-lock icons"></span> Pleadge Shares</p>
        <!-- <div class="table-pagination text-center">
            <ul class="pagination pagination-sm">
                <li>
                    <a href="#" aria-label="Previous">
                        <i class="fa fa-chevron-left icon"></i>
                    </a>
                </li>
                <li class="active"><a href="#">1</a></li>
                <li><a href="#">2</a></li>
                <li><a href="#">3</a></li>
                <li><a href="#">4</a></li>
                <li><a href="#">5</a></li>
                <li>
                    <a href="#" aria-label="Next">
                        <i class="fa fa-chevron-right icon"></i>
                    </a>
                </li>
            </ul>

        </div> -->
    </div>
</div>

<div class="modal fade" id="viewTradesPop" tabindex="-1" role="dialog" aria-labelledby="viewTradesPop">
    <div class="modal-dialog modal-md" role="document">
        <div class="modal-content">
            <div class="modal-body modal-padding ">
                <button type="button" class="btn remove" data-dismiss="modal" aria-label="Close">
                    <span class="fa fa-close"></span>
                </button>
                <div class="head-content"></div>
                <div class="symbols-modal-contant">
                    <div class="pricing-section">
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>