<div class="container">
    <div class="form-page clearfix">
        <div class="row pull-left">
            <div class="col-sm-2">
                <div class="form-group">
                    <label for="segmentYear" class="control-label">Statement</label>
                    <select name="" class="form-control" id="segmentYear">
                        <option value="2016-2017">Contract note</option>
                        <option value="2016-2017">Annual global statement</option>
                        <option value="2016-2017">Sauda bill</option>
                    </select>
                </div>
            </div>
            <div class="col-sm-2">
                <div class="form-group">
                    <label for="segmentYear" class="control-label">financial year</label>
                    <select name="" class="form-control" id="segmentYear">
                        <option value="2016-2017">2019 -20</option>
                        <option value="2016-2017">2020 -21</option>
                        <option value="2016-2017">2021 -22</option>
                    </select>
                </div>
            </div>

            <div class="col-sm-5">
                <div class="row">
                    <div class="col-sm-6">
                        <div class="form-group">
                            <label for="toDate" class="control-label">Start Date</label>
                            <div class="input-icon input-icon-right">
                                <i class="fa fa-calendar icons"></i>
                                <input type="text" class="form-control pickDate" id="toDate" placeholder="DD/MM/YYYY">
                            </div>
                        </div>
                    </div>
                    <div class="col-sm-6">
                        <div class="form-group">
                            <label for="toDate" class="control-label">End Date</label>
                            <div class="input-icon input-icon-right">
                                <i class="fa fa-calendar icons"></i>
                                <input type="text" class="form-control pickDate" id="fromDate" placeholder="DD/MM/YYYY">
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-sm-2">
                <div class="form-group">
                    <label for="segmentYear" class="control-label">Segment</label>
                    <select name="" class="form-control" id="">
                        <option value="">Equity</option>
                        <option value="">Derivative</option>
                        <option value="">Commodity</option>
                        <option value="">Currency</option>
                    </select>
                </div>
            </div>
            <div class="col-sm-1">
                <div class="form-group with-btn">
                    <button class="btn btn-theme hoverable waves-effect btn-block waves-light">Submit</button>
                </div>
            </div>
        </div>
